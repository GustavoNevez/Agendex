{
  "extends": ["react-app", "react-app/jest"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "semi": ["warn", "always"],
    "import/no-unresolved": "off" // Temporariamente desativando esta regra
  }
}
